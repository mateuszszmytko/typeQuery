{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [ "es2015", "dom", "es2015.promise" ],
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": false,
	  "baseUrl": ".",
	  "paths": {
		  "rq/*": ["src/*"]
	  }
  },
  "include": [
     "test/**/*",
     "src/**/*"
  ]
}